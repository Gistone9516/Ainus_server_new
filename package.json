{
  "name": "ainus-server",
  "version": "1.0.0",
  "description": "Ainus AI Model Analysis Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "ts-node -r tsconfig-paths/register src/scripts/migrate.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ai",
    "models",
    "analysis",
    "benchmark"
  ],
  "author": "Ainus Dev Team",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.4",
    "dompurify": "^3.0.6",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.9.0",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "^9.0.0",
    "marked": "^11.1.1",
    "mysql2": "^3.6.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^7.0.10",
    "rate-limit-redis": "^4.2.3",
    "redis": "^4.6.10",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
